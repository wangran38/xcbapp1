
.typeItem {
		width: 120rpx;
		height: 120rpx;
		font-size: 30rpx;
		background-color: white;
		text-align: center;
		line-height: 120rpx;
		border-radius: 20rpx;
		border: 1rpx solid gray;
		box-shadow: 10px 10px 10px -4px rgba(128, 128, 128, 0.5);
}
.title {
		margin: 10rpx;
		border-radius: 20rpx 200rpx;
		padding: 30rpx;
		font-size: 35rpx;
		font-weight: bold;
		color: white;
		background: linear-gradient(135deg, #ff6a00, #ff8229);
}

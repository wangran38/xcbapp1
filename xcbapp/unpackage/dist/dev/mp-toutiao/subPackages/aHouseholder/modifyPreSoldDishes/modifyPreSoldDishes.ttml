<view class="container"><view class="product-header"><text class="product-name">{{formData.goodsname}}</text><view class="decorative-line"></view></view><view class="form-container"><view class="form-item"><view class="item-header"><text class="label">预售价</text></view><view class="input-group"><text class="currency">¥</text><input class="price-input" type="number" placeholder="请输入价格" data-event-opts="{{[['input',[['__set_model',['$0','presaleprice','$event',['number']],['formData']]]],['blur',[['$forceUpdate']]]]}}" value="{{formData.presaleprice}}" bindinput="__e" bindblur="__e"/></view></view><view class="form-item"><view class="item-header"><text class="label">预售库存</text></view><input class="stock-input" type="number" placeholder="请输入库存量" data-event-opts="{{[['input',[['__set_model',['$0','goodstotal','$event',['number']],['formData']]]],['blur',[['$forceUpdate']]]]}}" value="{{formData.goodstotal}}" bindinput="__e" bindblur="__e"/></view><view class="form-item"><view class="item-header"><text class="label">商品单位</text></view><input class="stock-input" type="text" placeholder="请输入单位" data-event-opts="{{[['input',[['__set_model',['$0','unit','$event',[]],['formData']]]]]}}" value="{{formData.unit}}" bindinput="__e"/></view><view class="form-item switch-item"><view class="item-header"><text class="label">是否预售</text><switch checked="{{formData.ispresale==1?false:true}}" color="#7BCFA9" data-event-opts="{{[['change',[['changeSwitch',['$event']]]]]}}" bindchange="__e"></switch></view></view></view><view class="submit-container"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit-btn" bindtap="__e">保存修改</button></view></view>
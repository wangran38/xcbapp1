<view class="container"><view class="progress-steps"><view class="step-line"></view><view class="{{(('step-item')+' '+((setp>=1)?'active':''))}}"><view class="step-dot"></view><text class="step-text">基本信息</text></view><view class="{{(('step-item')+' '+((setp>=2)?'active':''))}}"><view class="step-dot"></view><text class="step-text">确认提交</text></view></view><scroll-view scroll-y="{{true}}"><view hidden="{{!(setp==1)}}" class="form-container"><view class="form-header"><uni-icons vue-id="bc4fa76e-1" type="info" size="20" color="#2979FF" bind:__l="__l"></uni-icons><text class="form-title">菜品信息</text></view><uni-forms class="vue-ref" vue-id="bc4fa76e-2" modelValue="{{formData}}" label-position="top" data-ref="form" bind:__l="__l" vue-slots="{{['default']}}"><view class="form-item"><label class="form-label">预售菜品</label><uni-easyinput bind:input="__e" vue-id="{{('bc4fa76e-3')+','+('bc4fa76e-2')}}" type="text" placeholder="请输入菜品名称" value="{{formData.goodsname}}" data-event-opts="{{[['^input',[['__set_model',['$0','goodsname','$event',[]],['formData']]]]]}}" bind:__l="__l"></uni-easyinput></view><view class="form-item"><label class="form-label">菜品分类</label><picker class="category-picker" range="{{pickerRange}}" value="{{selectedCategoryIndex}}" data-event-opts="{{[['change',[['onCategoryChange',['$event']]]]]}}" bindchange="__e"><view class="picker-content"><text>{{pickerRange[selectedCategoryIndex]}}</text><uni-icons vue-id="{{('bc4fa76e-4')+','+('bc4fa76e-2')}}" type="arrowdown" size="14" color="#666" bind:__l="__l"></uni-icons></view></picker></view><view class="form-item"><label class="form-label upload-label"><uni-icons vue-id="{{('bc4fa76e-5')+','+('bc4fa76e-2')}}" type="image" size="18" color="#2979FF" bind:__l="__l"></uni-icons><text>菜品图片</text></label><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="{{(('upload-area')+' '+((isImageSelected)?'has-image':''))}}" bindtap="__e"><uni-icons vue-id="{{('bc4fa76e-6')+','+('bc4fa76e-2')}}" type="{{isImageSelected?'checkmarkempty':'plusempty'}}" size="32" color="{{isImageSelected?'#67C23A':'#C0C4CC'}}" bind:__l="__l"></uni-icons><text class="upload-tip">{{isImageSelected?'上传成功':'点击上传'}}</text></view></view></uni-forms></view><view hidden="{{!(setp==2)}}" class="form-container"><view class="regulations"><view class="title">第一条 商品信息</view><view class="content"><view>1.1</view><view>1.2</view></view></view><view class="regulations"><view class="title">第二条 质量保证</view><view class="content"><view>2.1</view><view>2.2</view></view></view></view><button data-event-opts="{{[['tap',[['gotoSupplement',['$event']]]]]}}" style="color:#ffffff;background-color:#007aff;" bindtap="__e">{{BtnText}}</button></scroll-view></view>
.Stallholder {
  /* background-color: red; */
  display: flex;
  flex-direction: column;
  height: 75vh;
  /* overflow-y: auto; */
}
.Stallholder-content {
  padding: 10rpx;
  box-sizing: border-box;
  -webkit-column-count: 2;
          column-count: 2;
  -webkit-column-gap: 20rpx;
          column-gap: 20rpx;
}
.Stallholder-item {
  width: 100%;
  height: 480rpx;
  -webkit-column-break-inside: avoid;
          break-inside: avoid;
  margin-bottom: 20rpx;

.me-container{overflow:hidden;width:100%;box-sizing:border-box;padding:0rpx 40rpx 0 10rpx;color:#fff;z-index:1;background-color:#f8f8f8;display:flex;flex-direction:column;justify-content:flex-start;position:relative;height:calc(100vh -10rpx)}.disheslist{width:100%;height:100vh;display:flex;flex-direction:column;margin-top:20rpx}.type{margin-left:30rpx;padding:20rpx;height:200rpx;width:90%;display:flex;flex-direction:row;background-color:#fff;justify-content:flex-start;align-items:center;margin-bottom:30rpx;border-radius:10rpx;box-shadow:5rpx 5rpx 5rpx 5rpx rgba(0,0,0,.3)}.type-image{border:1px solid #0ff;width:200rpx;height:200rpx;border-radius:20rpx;background-color:#007aff}.regard{flex:1;height:180rpx;color:#000;font-size:30rpx;font-weight:600;margin-left:20rpx}.function{display:flex;height:100%;width:150rpx;margin-right:0;justify-content:end;align-items:center}.rigicon{height:70rpx;width:110rpx;background-color:#007aff;display:flex;justify-content:center;align-items:center;border-radius:30rpx}.disabled{background-color:#d3d3d3;cursor:not-allowed}.loading{text-align:center;padding:20rpx;color:#999}